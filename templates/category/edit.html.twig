{% extends "base.html.twig" %}

{% block title %}
	Modification de
	{{category.name}}
{% endblock %}

{% block body %}
	<h1>Modifier
		{{category.name}}</h1>

	{{form_start(formView)}}
	{{form_errors(formView)}}

	{{form_widget(formView)}}


	<button type="submit" class="btn btn-primary mt-3">
		<em class="fas fa-save"></em>
		Enregistrer
	</button>

	{{form_end(formView)}}
{% endblock %}
